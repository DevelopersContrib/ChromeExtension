{
    "name": "VNOCTrackers",
    "version": "0.0.1",
    "manifest_version": 2,
    
	"browser_action": {
	"default_icon": "icon.png",
        "default_title": "Trackers",
        "default_popup": "popup.html"
    },
	"permissions": [
    "activeTab",
    "<all_urls>"
],
	"content_scripts" : [
		{
			"matches": 	["http://*/*", "https://*/*"],
			"css":		["jquery-ui.css","custom.css"],
			"js" : 		["jquery-1.12.4.js","jquery-ui.js"]
		}
	]
}
