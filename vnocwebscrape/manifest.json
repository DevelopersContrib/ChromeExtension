{
    "name": "VNOCTrackers",
    "version": "0.0.1",
    "manifest_version": 2,
    
	"browser_action": {
	"default_icon": "icon.png",
        "default_title": "Trackers",
        "default_popup": "popup.html"
    },
	"icons": { "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png" },
		"permissions": [
		"activeTab",
		"<all_urls>"
	],
	"content_scripts" : [
		{
			"matches": 	["http://*/*", "https://*/*"],
			"css":		["custom.css"],
			"js" : 		["jquery-3.3.1.js","jquery-ui.js"]
		}
	]
}
